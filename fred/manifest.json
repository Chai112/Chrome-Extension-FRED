{
  "name": "Facial Recognition of Emotion Detector",
  "version": "1.0",
  "description": "FRED",
  "permissions": ["tabs", "management", "storage", "activeTab"],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "browser_action": { // change to page_action to disable by default
  	"default_popup": "popup.html",
  	"default_icon": {
        "48": "images/fred48.png",
        "128": "images/fred128.png"
      }
  },
  "icons": {
    "48": "images/fred48.png",
    "128": "images/fred128.png"
  },
  "manifest_version": 2
}
